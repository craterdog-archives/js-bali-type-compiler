[
    $parameters: []
    $variables: [
        $$result
        $no
        $target
        $yes
    ]
    $procedures: [$asBoolean]
    $addresses: [
        "1.IfStatement": 1
        "1.1.ConditionClause": 2
        "1.1.1.EvaluateStatement": 5
        "1.1.ConditionClauseDone": 7
        "1.2.ElseClause": 8
        "1.2.1.EvaluateStatement": 9
        "1.2.ElseClauseDone": 11
        "1.IfStatementDone": 12
    ]
    $instructions: "
        1.IfStatement:
        SKIP INSTRUCTION
        
        1.1.ConditionClause:
        PUSH LITERAL `true`
        EXECUTE $asBoolean ON TARGET
        JUMP TO 1.2.ElseClause ON FALSE
        
        1.1.1.EvaluateStatement:
        LOAD VARIABLE $yes
        STORE VARIABLE $$result
        
        1.1.ConditionClauseDone:
