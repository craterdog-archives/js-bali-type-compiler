[
    $procedure: {
        if true then {
            yes
        } else {
            no
        }
    }
    $instructions: "
        1.IfStatement:
        SKIP INSTRUCTION
        
        1.1.ConditionClause:
        PUSH LITERAL `true`
        JUMP TO 1.2.ElseClause ON FALSE
        
        1.1.1.EvaluateStatement:
        LOAD VARIABLE $yes
        STORE VARIABLE $result-1
        
        1.1.ConditionClauseDone:
        JUMP TO 1.IfStatementDone
        
        1.2.ElseClause:
        SKIP INSTRUCTION
        
        1.2.1.EvaluateStatement:
        LOAD VARIABLE $no
        STORE VARIABLE $result-1
        
        1.2.ElseClauseDone:
        SKIP INSTRUCTION
        
        1.IfStatementDone:
        LOAD VARIABLE $result-1
        HANDLE RESULT
    "($mediaType: "application/basm")
    $addresses: [
        "1.IfStatement": 1
        "1.1.ConditionClause": 2
        "1.1.1.EvaluateStatement": 4
        "1.1.ConditionClauseDone": 6
        "1.2.ElseClause": 7
        "1.2.1.EvaluateStatement": 8
        "1.2.ElseClauseDone": 10
        "1.IfStatementDone": 11
    ]
    $bytecode: '00002801180760038002000B00006001800200006002E000'(
        $encoding: $base16
        $mediaType: "application/bcod"
    )
    $arguments: [
        $target
    ]
    $variables: [
        $no
        $result-1
        $yes
    ]($type: /bali/collections/Set/v1)
    $messages: [ ]($type: /bali/collections/Set/v1)
]
