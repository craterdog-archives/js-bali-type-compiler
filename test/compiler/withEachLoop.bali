[
    $source: {
        with each $symbol in $sequence do {
            $x := true
            if x then {
                $y := none
                continue loop
            }
            $z := any
        }
    }
    $instructions: "
        1.WithStatement:
        PUSH LITERAL `$sequence`
        SEND $iterator TO COMPONENT
        STORE VARIABLE $$iterator-1
        
        1.1.ConditionClause:
        LOAD VARIABLE $$iterator-1
        SEND $hasNext TO COMPONENT
        JUMP TO 1.WithStatementDone ON FALSE
        LOAD VARIABLE $$iterator-1
        SEND $next TO COMPONENT
        STORE VARIABLE $symbol
        
        1.1.1.EvaluateStatement:
        PUSH LITERAL `true`
        STORE VARIABLE $x
        
        1.1.2.IfStatement:
        SKIP INSTRUCTION
        
        1.1.2.1.ConditionClause:
        LOAD VARIABLE $x
        JUMP TO 1.1.2.IfStatementDone ON FALSE
        
        1.1.2.1.1.EvaluateStatement:
        PUSH LITERAL `none`
        STORE VARIABLE $y
        
        1.1.2.1.2.ContinueStatement:
        JUMP TO 1.1.ConditionClause
        
        1.1.2.IfStatementDone:
        SKIP INSTRUCTION
        
        1.1.3.EvaluateStatement:
        PUSH LITERAL `any`
        STORE VARIABLE $z
        
        1.ConditionRepeat:
        JUMP TO 1.1.ConditionClause
        
        1.WithStatementDone:
        LOAD VARIABLE $$result
        HANDLE RESULT
    "($mediatype: "application/basm")
    $bytecode: '
        2801C00280016001C00118166001C0038003280280050000600518122803
        8006000400002804800700046002E000
    '(
        $encoding: $base16
        $mediatype: "application/bcod"
    )
