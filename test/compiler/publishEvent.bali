[
    $procedure: {
        publish [
            $type: $changeEvent
            $key: $foo
            $value: 5
        ]
    }
    $instructions: "
        1.PublishStatement:
        CALL $catalog
        PUSH LITERAL `$type`
        PUSH LITERAL `$changeEvent`
        CALL $association WITH 2 ARGUMENTS
        CALL $addItem WITH 2 ARGUMENTS
        PUSH LITERAL `$key`
        PUSH LITERAL `$foo`
        CALL $association WITH 2 ARGUMENTS
        CALL $addItem WITH 2 ARGUMENTS
        PUSH LITERAL `$value`
        PUSH LITERAL `5`
        CALL $association WITH 2 ARGUMENTS
        CALL $addItem WITH 2 ARGUMENTS
        PUSH LITERAL `/bali/events/bag`
        SAVE VARIABLE $bag-2
        SAVE MESSAGE $bag-2
        LOAD VARIABLE $result-1
        PULL RESULT
    "($mediaType: "application/basm")
    $addresses: [
        "1.PublishStatement": 1
    ]
    $bytecode: '
        C01428012802D00BD00128032804D00BD00128052806D00BD00128078001
        880160025000
    '(
        $encoding: $base16
        $mediaType: "application/bcod"
    )
    $arguments: [
        $target
    ]
    $variables: [
        $bag-2
        $result-1
    ]($type: /bali/collections/Set/v1)
    $messages: [ ]($type: /bali/collections/Set/v1)
]
