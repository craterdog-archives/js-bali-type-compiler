[
    $procedure: {
        checkout $document from /acme/documents/example/v1.2
        save document
        commit document to /acme/documents/example/v1.3
        checkout level 3 of documents[index] from /acme/documents/example/v1.3
        save documents[index]
        commit documents[index] to /acme/documents/example/v1.3.1
        discard documents[index]
    }
    $instructions: "
        1.CheckoutStatement:
        ---- Save the name of the document.
        PUSH LITERAL `/acme/documents/example/v1.2`
        SAVE VARIABLE $name-2
        ---- Load a draft copy of the named document from the repository.
        LOAD DOCUMENT $name-2
        CALL $duplicate WITH 1 ARGUMENT
        SAVE VARIABLE $draft-3
        ---- Calculate the new version string for the draft and save it.
        LOAD VARIABLE $draft-3
        PUSH LITERAL `$version`
        CALL $parameter WITH 2 ARGUMENTS
        PUSH LITERAL `0`
        CALL $nextVersion WITH 2 ARGUMENTS
        SAVE VARIABLE $version-4
        ---- Set the new version string parameter for the draft document.
        LOAD VARIABLE $draft-3
        PUSH LITERAL `$version`
        LOAD VARIABLE $version-4
        CALL $setParameter WITH 3 ARGUMENTS
        ---- Set the new draft document as the value of the recipient.
        PUSH LITERAL `$document`
        LOAD VARIABLE $draft-3
        ---- Save the value of the expression in a variable.
        SAVE VARIABLE $document
        
        2.SaveStatement:
        LOAD VARIABLE $document
        CALL $citation WITH 1 ARGUMENT
        SAVE VARIABLE $citation-5
        SAVE DRAFT $citation-5
        
        3.CommitStatement:
        ---- Save the name of the document.
        PUSH LITERAL `/acme/documents/example/v1.3`
        SAVE VARIABLE $name-6
        ---- Commit the named document to the repository.
        LOAD VARIABLE $document
        SAVE DOCUMENT $name-6
        
        4.CheckoutStatement:
        ---- Save the name of the document.
        PUSH LITERAL `/acme/documents/example/v1.3`
        SAVE VARIABLE $name-7
        ---- Load a draft copy of the named document from the repository.
        LOAD DOCUMENT $name-7
        CALL $duplicate WITH 1 ARGUMENT
        SAVE VARIABLE $draft-8
        ---- Calculate the new version string for the draft and save it.
        LOAD VARIABLE $draft-8
        PUSH LITERAL `$version`
        CALL $parameter WITH 2 ARGUMENTS
        PUSH LITERAL `3`
        CALL $nextVersion WITH 2 ARGUMENTS
        SAVE VARIABLE $version-9
        ---- Set the new version string parameter for the draft document.
        LOAD VARIABLE $draft-8
        PUSH LITERAL `$version`
        LOAD VARIABLE $version-9
        CALL $setParameter WITH 3 ARGUMENTS
        ---- Set the new draft document as the value of the recipient.
        LOAD VARIABLE $documents
        LOAD VARIABLE $index
        LOAD VARIABLE $draft-8
        ---- Set the subcomponent at the given index to the value of the expression.
        CALL $setSubcomponent WITH 3 ARGUMENTS
        
        5.SaveStatement:
        LOAD VARIABLE $documents
        LOAD VARIABLE $index
        CALL $subcomponent WITH 2 ARGUMENTS
        CALL $citation WITH 1 ARGUMENT
        SAVE VARIABLE $citation-10
        SAVE DRAFT $citation-10
        
        6.CommitStatement:
        ---- Save the name of the document.
        PUSH LITERAL `/acme/documents/example/v1.3.1`
        SAVE VARIABLE $name-11
        ---- Commit the named document to the repository.
        LOAD VARIABLE $documents
        LOAD VARIABLE $index
        CALL $subcomponent WITH 2 ARGUMENTS
        SAVE DOCUMENT $name-11
        
        7.DiscardStatement:
        ---- Place the draft document on the stack.
        LOAD VARIABLE $documents
        LOAD VARIABLE $index
        CALL $subcomponent WITH 2 ARGUMENTS
        ---- Save a citation to the draft document.
        CALL $citation WITH 1 ARGUMENT
        SAVE VARIABLE $citation-12
        ---- Drop the cited draft document from the repository.
        DROP DRAFT $citation-12
        LOAD VARIABLE $result-1
        PULL RESULT
    "($mediaType: "application/basm")
