[
    $procedure: {
        checkout $document from /acme/documents/example/v1.2
        save document
        commit document to /acme/documents/example/v1.3
        checkout level 3 of documents[index] from /acme/documents/example/v1.3
        save documents[index] as citations[index]
        commit documents[index] to /acme/documents/example/v1.3.1
        discard documents[index]
    }
    $instructions: "
        1.CheckoutStatement:
        ---- Save the name of the contract.
        PUSH LITERAL `/acme/documents/example/v1.2`
        SAVE REGISTER $name-2
        ---- Load a copy of the named contract from the repository.
        LOAD CONTRACT $name-2
        CALL $duplicate WITH 1 ARGUMENT
        SAVE REGISTER $document-3
        ---- Calculate the new version string for the new document and save it.
        LOAD REGISTER $document-3
        PUSH LITERAL `$version`
        CALL $parameter WITH 2 ARGUMENTS
        PUSH LITERAL `0`
        CALL $nextVersion WITH 2 ARGUMENTS
        SAVE REGISTER $version-4
        ---- Set the new version string parameter for the new document.
        LOAD REGISTER $document-3
        PUSH LITERAL `$version`
        LOAD REGISTER $version-4
        CALL $setParameter WITH 3 ARGUMENTS
        PULL COMPONENT
        ---- Set the new document as the value of the recipient.
        LOAD REGISTER $document-3
        SAVE REGISTER $document
        
        2.SaveStatement:
        ---- Place the document on the stack.
        LOAD REGISTER $document
        ---- Save the document to the repository and a citation to it.
        SAVE DOCUMENT $citation-5
        
        3.CommitStatement:
        ---- Save the name of the new contract.
        PUSH LITERAL `/acme/documents/example/v1.3`
        SAVE REGISTER $name-6
        ---- Commit the named contract to the repository.
        LOAD REGISTER $document
        SAVE CONTRACT $name-6
        
        4.CheckoutStatement:
        ---- Save the name of the contract.
        PUSH LITERAL `/acme/documents/example/v1.3`
        SAVE REGISTER $name-7
        ---- Load a copy of the named contract from the repository.
        LOAD CONTRACT $name-7
        CALL $duplicate WITH 1 ARGUMENT
        SAVE REGISTER $document-8
        ---- Calculate the new version string for the new document and save it.
        LOAD REGISTER $document-8
        PUSH LITERAL `$version`
        CALL $parameter WITH 2 ARGUMENTS
        PUSH LITERAL `3`
        CALL $nextVersion WITH 2 ARGUMENTS
        SAVE REGISTER $version-9
        ---- Set the new version string parameter for the new document.
        LOAD REGISTER $document-8
        PUSH LITERAL `$version`
        LOAD REGISTER $version-9
        CALL $setParameter WITH 3 ARGUMENTS
        PULL COMPONENT
        ---- Set the new document as the value of the recipient.
        ---- Place the recipient and the index of its subcomponent on the stack.
        LOAD REGISTER $documents
        LOAD REGISTER $index
        LOAD REGISTER $document-8
        ---- Assign the result as the value of the subcomponent.
        CALL $setSubcomponent WITH 3 ARGUMENTS
        PULL COMPONENT
        
        5.SaveStatement:
        ---- Place the document on the stack.
        LOAD REGISTER $documents
        LOAD REGISTER $index
        CALL $subcomponent WITH 2 ARGUMENTS
        ---- Save the document to the repository and a citation to it.
        SAVE DOCUMENT $citation-10
        ---- Place the recipient and the index of its subcomponent on the stack.
        LOAD REGISTER $citations
        LOAD REGISTER $index
        LOAD REGISTER $citation-10
        ---- Assign the result as the value of the subcomponent.
        CALL $setSubcomponent WITH 3 ARGUMENTS
        PULL COMPONENT
        
        6.CommitStatement:
        ---- Save the name of the new contract.
        PUSH LITERAL `/acme/documents/example/v1.3.1`
        SAVE REGISTER $name-11
        ---- Commit the named contract to the repository.
        LOAD REGISTER $documents
        LOAD REGISTER $index
        CALL $subcomponent WITH 2 ARGUMENTS
        SAVE CONTRACT $name-11
        
        7.DiscardStatement:
        ---- Save the citation to the document.
        LOAD REGISTER $documents
        LOAD REGISTER $index
        CALL $subcomponent WITH 2 ARGUMENTS
        SAVE REGISTER $citation-12
        ---- Drop the cited document from the repository.
        DROP DOCUMENT $citation-12
        LOAD REGISTER $result-1
        PULL RESULT
    "($mediaType: "application/basm")
    $addresses: [
        "1.CheckoutStatement": 1
        "2.SaveStatement": 19
        "3.CommitStatement": 21
        "4.CheckoutStatement": 25
        "5.SaveStatement": 46
        "6.CommitStatement": 55
        "7.DiscardStatement": 61
    ]
    $bytecode: '
        2801800B700BC829800660062802D0602803D05B800F60062802600FD882
        480060068005600588032805800C6005900C2805800D700DC82980076007
        2802D0602808D05B8010600728026010D8824800600860096007D8834800
        60086009D08B8801600460096001D8834800280A800A60086009D08B900A
        60086009D08B8002A802600E5000
    '(
        $encoding: $base16
        $mediaType: "application/bcod"
    )
    $arguments: [
        $target
    ]
    $variables: [
        $citation-10
        $citation-12
        $citation-5
        $citations
        $document
        $document-3
        $document-8
        $documents
        $index
        $name-11
        $name-2
