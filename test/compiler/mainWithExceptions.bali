[
    $procedure: {
        post message on <bali:/queues/myqueue> handle $error matching [
            $type: "bad"
        ] with {
            any
        } matching [
            $type: "horrid"
        ] with {
            none
        }
    }
    $instructions: "
        1.PostStatement:
        PUSH HANDLER 1.PostStatementHandler
        PUSH LITERAL `<bali:/queues/myqueue>`
        STORE VARIABLE $queue-2
        LOAD VARIABLE $message
        STORE MESSAGE $queue-2
        
        1.PostStatementDone:
        POP HANDLER
        JUMP TO 1.PostStatementSucceeded
        
        1.PostStatementHandler:
        STORE VARIABLE $error
        
        1.1.HandleBlock:
        LOAD VARIABLE $error
        INVOKE $catalog
        PUSH LITERAL `$type`
        PUSH LITERAL `"bad"`
        INVOKE $association WITH 2 ARGUMENTS
        INVOKE $addItem WITH 2 ARGUMENTS
        INVOKE $doesMatch WITH 2 ARGUMENTS
        JUMP TO 1.2.HandleBlock ON FALSE
        
        1.1.1.EvaluateStatement:
        PUSH LITERAL `any`
        STORE VARIABLE $result-1
        
        1.1.HandleBlockDone:
        JUMP TO 1.PostStatementSucceeded
        
        1.2.HandleBlock:
        LOAD VARIABLE $error
        INVOKE $catalog
        PUSH LITERAL `$type`
        PUSH LITERAL `"horrid"`
        INVOKE $association WITH 2 ARGUMENTS
        INVOKE $addItem WITH 2 ARGUMENTS
        INVOKE $doesMatch WITH 2 ARGUMENTS
        JUMP TO 1.PostStatementFailed ON FALSE
        
        1.2.1.EvaluateStatement:
        PUSH LITERAL `none`
        STORE VARIABLE $result-1
        
        1.2.HandleBlockDone:
        JUMP TO 1.PostStatementSucceeded
        
        1.PostStatementFailed:
        LOAD VARIABLE $error
        HANDLE EXCEPTION
        
        1.PostStatementSucceeded:
        LOAD VARIABLE $result-1
        HANDLE RESULT
    "($mediaType: "application/basm")
    $addresses: [
        "1.PostStatement": 1
        "1.PostStatementDone": 6
        "1.PostStatementHandler": 8
        "1.1.HandleBlock": 9
        "1.1.1.EvaluateStatement": 17
        "1.1.HandleBlockDone": 19
        "1.2.HandleBlock": 20
        "1.2.1.EvaluateStatement": 28
        "1.2.HandleBlockDone": 30
        "1.PostStatementFailed": 31
        "1.PostStatementSucceeded": 33
    ]
    $bytecode: '
        200828018003600288034000002180016001A01428022803B00BB001B027
        18142804800400216001A01428022806B00BB001B027181F280780040021
        6001E8006004E000
    '(
        $encoding: $base16
        $mediaType: "application/bcod"
    )
    $arguments: [
        $target
    ]
    $variables: [
        $error
        $message
        $queue-2
        $result-1
    ]($type: /bali/collections/Set/v1)
    $messages: [ ]($type: /bali/collections/Set/v1)
]
