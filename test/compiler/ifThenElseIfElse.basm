[
    $parameters: []
    $variables: [
        $$result
        $no
        $target
        $yes
    ]
    $procedures: [$asBoolean]
    $addresses: [
        "1.IfStatement": 1
        "1.1.ConditionClause": 2
        "1.1.1.EvaluateStatement": 5
        "1.1.ConditionClauseDone": 7
        "1.2.ConditionClause": 8
        "1.2.1.EvaluateStatement": 11
        "1.2.ConditionClauseDone": 13
        "1.3.ElseClause": 14
        "1.3.1.EvaluateStatement": 15
        "1.3.ElseClauseDone": 17
        "1.IfStatementDone": 18
    ]
    $instructions: "
        1.IfStatement:
        SKIP INSTRUCTION
        
        1.1.ConditionClause:
        PUSH LITERAL `true`
        EXECUTE $asBoolean ON TARGET
        JUMP TO 1.2.ConditionClause ON FALSE
        
        1.1.1.EvaluateStatement:
        LOAD VARIABLE $yes
        STORE VARIABLE $$result
        
        1.1.ConditionClauseDone:
        JUMP TO 1.IfStatementDone
        
