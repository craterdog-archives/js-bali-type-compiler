[
    $procedure: {
        post [
            $foo: "bar"
        ] to /acme/messages/inbound/v1.2.3
    }
    $instructions: "
        1.PostStatement:
        ---- Save the name of the message bag.
        PUSH LITERAL `/acme/messages/inbound/v1.2.3`
        SAVE REGISTER $bag-2
        ---- Post a message to the named message bag.
        CALL $catalog
        ---- Add an item to the catalog.
        PUSH LITERAL `$foo`
        PUSH LITERAL `"bar"`
        CALL $association WITH 2 ARGUMENTS
        CALL $addItem WITH 2 ARGUMENTS
        SAVE MESSAGE $bag-2
        LOAD REGISTER $result-1
        PULL RESULT
    "($mediaType: "application/basm")
    $addresses: [
        "1.PostStatement": 1
    ]
    $bytecode: '28018001C01428022803D00BD001980160025000'(
        $encoding: $base16
        $mediaType: "application/bcod"
    )
    $arguments: [
        $target
    ]
    $variables: [
        $bag-2
        $result-1
    ]($type: /bali/collections/Set/v1)
    $messages: [ ]($type: /bali/collections/Set/v1)
]
