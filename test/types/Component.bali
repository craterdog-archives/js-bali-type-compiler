        $isA: [
            $description: "
                This procedure determines whether or not this component is an instance
                of the specified type.
            "($mediatype: "application/bsmd")
            $parameters: [
                $type: [
                    $description: "
                        The component type to be compared with.
                    "($mediatype: "application/bsmd")
