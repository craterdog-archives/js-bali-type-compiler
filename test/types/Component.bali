                ]
            ]
            $type: <bali:/Probability>($granularity: $boolean)
            $source: {
                $ancestors := @getType(target).getAncestors()
                with each $ancestor in ancestors do {
                    if ancestor = type then {
                        return true
                    }
                }
                return false
            }($type)
        ]
        $getAncestry: [
            $description: "
                This procedure returns a list of references to the ancestors for the
                type of this component.
            "($mediatype: "application/bsmd")
            $type: <bali:/List>(
                $ItemType: <bali:/Reference>(
                    $referencedType: <bali:/Type>
                )
            )
            $source: {
                $type := getType(target)
                $ancestors := [type]
                ancestors.addItems(@type[$ancestors])
                return ancestors
            }
        ]
