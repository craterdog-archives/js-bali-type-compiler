[
    $description: "
        This library defines the procedures for the functions that operate on any type of component.
    "($mediatype: "application/bsmd")
    $constants: [:]
    $procedures: [
        $ancestry: [
            $description: "
                This procedure returns a list of the type names for the ancestors of this type of
                component.
            "($mediatype: "application/bsmd")
            $type: /bali/collections/List/v1(
                $itemType: /bali/elements/Name/v1
            )
            $source: {
                $ancestry := []
                $current := type(component)
                while current do {
                    ancestry.addItem(current)
                    $current := @(current[parent])
                }
                return ancestry
            }($component: /bali/abstractions/Component/v1)
        ]
        $areEqual: [
            $description: "
                This procedure determines whether or not two components are equal to each other.
            "($mediatype: "application/bsmd")
            $type: /bali/elements/Probability/v1($granularity: $boolean)
            $source: {
                return areEqual(first, second)
            }(
                $first: /bali/abstractions/Component/v1
                $second: /bali/abstractions/Component/v1
            )
        ]
        $areSame: [
            $description: "
                This procedure determines whether or not two components are really the same
                component.
            "($mediatype: "application/bsmd")
            $type: /bali/elements/Probability/v1($granularity: $boolean)
            $source: {
                return areSame(first, second)
            }(
                $first: /bali/abstractions/Component/v1
                $second: /bali/abstractions/Component/v1
            )
        ]
        $boolean: [
            $description: "
                This procedure returns a boolean value describing whether or not the component has
                a meaningful value.
            "($mediatype: "application/bsmd")
            $type: /bali/elements/Probability/v1($granularity: $boolean)
            $source: {
                return boolean(component)
            }($component: /bali/abstractions/Component/v1)
        ]
        $comparison: [
            $description: "
                This procedure returns a tertiary number describing the natural ordering for two
                components:
                 -1: The first component should be listed before the second component.
                 0: The first component should be listed the same as the second component.
                 1: The first component should be listed after the second component.
            "($mediatype: "application/bsmd")
            $type: /bali/elements/Number/v1($range: [-1..1])
            $source: {
                return comparison(first, second)
            }(
                $first: /bali/abstractions/Component/v1
                $second: /bali/abstractions/Component/v1
            )
        ]
        $document: [
            $description: "
                This procedure returns the Bali Document Notationâ„¢ for a component.  If an
                indentation is specified, each line of the resulting document will be indented by
                that number of additional levels.
            "($mediatype: "application/bsmd")
            $type: /bali/elements/Text/v1($mediatype: "application/bali")
            $source: {
                return document(component, indentation)
            }(
                $component: /bali/abstractions/Component/v1
                $indentation: /bali/elements/Number/v1($range: [0..20])
            )
        ]
        $doesMatch: [
            $description: "
                This procedure determines whether or not a component matches a pattern.
            "($mediatype: "application/bsmd")
            $type: /bali/elements/Probability/v1($granularity: $boolean)
            $source: {
                return doesMatch(component, pattern)
            }(
                $component: /bali/abstractions/Component/v1
                $pattern: /bali/elements/Pattern/v1
            )
        ]
        $duplicate: [
            $description: "
                This procedure returns a (deep) copy of a component.
            "($mediatype: "application/bsmd")
            $type: /bali/abstractions/Component/v1
            $source: {
                return duplicate(component)
            }($component: /bali/abstractions/Component/v1)
        ]
        $hash: [
            $description: "
                This procedure returns the canonical hash value for a component.
            "($mediatype: "application/bsmd")
            $type: /bali/elements/Number/v1($range: [0..4294967295])
            $source: {
                return hash(component)
            }($component: /bali/abstractions/Component/v1)
        ]
        $HTML: [
            $description: "
                This procedure returns a text string containing the corresponding HTML code for
                a component.
            "($mediatype: "application/bsmd")
            $type: /bali/elements/Text/v1($mediatype: "text/html")
            $source: {
                return HTML(component)
            }($component: /bali/abstractions/Component/v1)
        ]
        $isA: [
            $description: "
                This procedure determines whether or not a component is an instance of the
                specified type or any of its ancestors.
            "($mediatype: "application/bsmd")
            $type: /bali/elements/Probability/v1($granularity: $boolean)
            $source: {
                $current := type(component)
                while current do {
                    if current = type then {
                        return true
                    }
                    $current := @(current[parent])
                }
                return false
            }(
                $component: /bali/abstractions/Component/v1
                $type: /bali/elements/Name/v1
            )
        ]
    ]
](
